  # Pods for Street Vendor Helper

  target 'Street Vendor HelperTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Street Vendor HelperUITests' do
    inherit! :search_paths
    # Pods for testing
  end

pod 'FacebookCore'
